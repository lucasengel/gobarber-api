{
  "name": "gobarber",
  "version": "1.0.0",
  "description": "Appointment management system for barbershops",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev --inspect --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "mig:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -n",
    "mig:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert",
    "mig:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run"
  },
  "keywords": [],
  "author": "Lucas Engel",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
