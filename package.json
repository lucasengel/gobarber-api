{
  "name": "gobarber",
  "version": "1.0.0",
  "description": "Appointment management system for barbershops",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --respawn --ignore-watch node_modules src/shared/infra/http/server.ts",
    "mig:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "mig:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "mig:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "start": "ts-node -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "Lucas Engel",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/mongodb": "^3.5.27",
    "@types/nodemailer": "^6.4.0",
    "aws-sdk": "^2.771.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.3",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "mongodb": "^3.6.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.13",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
